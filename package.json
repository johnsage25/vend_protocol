    "dev": "next",
    "lint": "tsc && eslint --cache 'src/**/*.{ts,tsx,js}' --max-warnings 0",
    "start": "next start",
    "test-docs": "bash scripts/test-docs.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/next-eui-starter"
  },
  "bugs": {
    "url": "https://github.com/elastic/next-eui-starter/issues"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^40.1.0",
    "@next/bundle-analyzer": "^12.0.2",
    "@react-pdf/renderer": "^2.0.20",
    "@rooks/use-window-size": "^4.11.2",
    "animejs": "^3.2.1",
    "echarts": "^5.2.1",
    "echarts-for-react": "^3.0.2",
    "glob": "^7.1.6",
    "moment": "^2.29.1",
    "next": "12.0.2",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
